{
  "name": "blueprint",
  "version": "0.0.0",
  "description": "Created with choo-cli",
  "license": "MIT",
  "scripts": {
    "start": "budo ./src/client.js --pushstate --live --dir=app -- -g es2040",
    "lint": "standard --verbose | snazzy",
    "test": "nyc ava **/*.test.js --watch",
    "report": "nyc report --reporter=html --watch"
  },
  "dependencies": {
    "choo": "^3.2.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "browserify": "^13.0.1",
    "budo": "8.3.0",
    "es2040": "1.2.2",
    "jsdom": "^9.4.2",
    "nyc": "^8.1.0",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2"
  },
  "standard": {
    "ignore": [
      "scripts"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "test.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "require": [
      "./test-helper.js",
      "babel-register"
    ],
    "babel": "inherit"
  }
}
