{
  "name": "blueprint",
  "version": "0.0.0",
  "description": "Created with choo-cli",
  "license": "MIT",
  "scripts": {
    "start": "budo ./src/client.js --dir=. --pushstate --live --onupdate \"standard\" -- -t [ sheetify/transform -u sheetify-cssnext ] -t es2040",
    "lint": "standard --verbose | snazzy",
    "test": "nyc ava **/*.test.js --watch",
    "report": "nyc report --reporter=html"
  },
  "dependencies": {
    "basscss": "^8.0.2",
    "choo": "^3.2.2",
    "choo-log": "^1.4.0",
    "sheetify": "^5.1.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "browserify": "^13.0.1",
    "budo": "8.3.0",
    "es2040": "1.2.2",
    "jsdom": "^9.4.2",
    "nyc": "^8.1.0",
    "sheetify-cssnext": "^1.0.7",
    "simulant": "^0.2.2",
    "sinon": "^1.17.5",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "test.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "require": [
      "./test-helper.js",
      "babel-core/register"
    ],
    "babel": "inherit"
  }
}
